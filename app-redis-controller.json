{
    "apiVersion": "v1beta3",
    "kind": "ReplicationController",
    "metadata": {
        "labels": {
            "name": "app-redis"
        },
        "name": "app-redis"
    },
    "spec": {
        "replicas": 1,
        "selector": {
            "name": "app-redis"
        },
        "template": {
            "metadata": {
                "labels": {
                    "name": "app-redis"
                }
            },
            "spec": {
                "containers": [
                    {
                        "capabilities": {},
                        "image": "dockerfile/redis",
                        "name": "app-redis",
                        "ports": [
                            {
                                "containerPort": 6379,
                                "protocol": "TCP"
                            }
                        ]
                    }
                ]
            }
        }
    }
}
